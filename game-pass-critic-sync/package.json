{
  "name": "@game-pass-critic/sync",
  "version": "1.0.0",
  "description": "Game pass critic - Sync",
  "main": "src/index.ts",
  "author": "penny-five",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@google-cloud/functions-framework": "1.7.1",
    "pino": "6.9.0"
  },
  "devDependencies": {
    "concurrently": "5.3.0",
    "env-cmd": "10.1.0",
    "nodemon": "2.0.6",
    "rimraf": "3.0.2",
    "ts-loader": "8.0.13",
    "typescript": "4.1.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "2.5.2"
  },
  "scripts": {
    "build": "rimraf dist/**/* && webpack",
    "serve": "nodemon --watch dist --exec \"env-cmd --silent functions-framework --source=dist --target default --signature-type=event\"",
    "dev": "NODE_ENV=development concurrently \"yarn build --watch\" \"yarn serve\""
  }
}
